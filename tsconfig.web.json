{
    "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
    "include": [
        "src/frontend/env.d.ts",
        "src/frontend/**/*",
        "src/frontend/**/*.tsx",
        "src/preload/*.d.ts",
        "src/shared/**/*"
    ],
    "compilerOptions": {
        "composite": true,
        "jsx": "react-jsx",
        "baseUrl": ".",
        "paths": {
            "@frontend/*": ["src/frontend/*"],
            "@shared/*": ["src/shared/*"],
            "@/*": ["src/frontend/*"]
        }
    }
}
